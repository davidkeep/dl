
@import { 
	'Expr.dl',
}

Call struct
{
    super Expr
    operand Expr^
    func Expr^
    params ExpressionList
}

Call fn() Call^
{
    self Call^ = new(Call)
    self.super.kind = Id(Call)
    self.params.Init()
    return self
}


Access struct {
	super Expr

	operand Expr^

	field Expr^
}

Init fn(self ref Access) {
	self.super.kind = Id(Access)
}
