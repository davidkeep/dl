@import {
	'dl/String.dl'
}

Gen struct (A?, B?)
{
	a A
	b B
}

Overload fn(a i32) i32 {
	return 0
}
Overload fn(a i64) i32 {
	return 1
}
Overload fn(type i32) i32 {
	return 2
}
Overload fn(type i64) i32 {
	return 3
}

Generic0 fn() i8 {
	a Gen(i32, i64)
	Assert(Overload(a.a) == 0)
	Assert(Overload(a.b) == 1)

	Assert(Overload(i32) == 2)
	Assert(Overload(i64) == 2)

	return 0
}
