@extern 
{
    cosf fn(v f32) f32
    sinf fn(v f32) f32
    tanf fn(v f32) f32

    fabsf fn(v f32) f32
    sqrtf fn(v f32) f32
}

PI f32 = 3.1415926535897

sqrt fn(f f32) f32 
{
	return sqrtf(f)
}
abs fn(f f32) f32 
{
	return fabsf(f)
}
cos fn(f f32) f32 
{
	return cosf(f)
}
sin fn(f f32) f32 
{
	return sinf(f)
}
tan fn(f f32) f32 
{
	return tanf(f)
}
